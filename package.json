{
  "name": "md-talk",
  "version": "0.5.0",
  "description": "Export AI Agents' sessions to Markdown",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx watch index.ts",
    "test": "vitest",
    "prepare": "npm run build"
  },
  "bin": {
    "md-talk": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.9"
  },
  "author": "jel1yspot <jel1yspot@outlook.com>",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/jel1yspot/md-talk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jel1yspot/md-talk.git"
  },
  "bugs": {
    "url": "https://github.com/jel1yspot/md-talk/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "codex",
    "claude",
    "gemini",
    "ai",
    "agent",
    "code",
    "chatgpt",
    "markdown",
    "cli",
    "session"
  ]
